{
  "name": "react-avancado-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "test:e2e:ci": "start-server-and-test start 3000 cy:run",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@stripe/react-stripe-js": "^1.10.0",
    "@stripe/stripe-js": "^1.35.0",
    "@types/lodash.xor": "^4.5.6",
    "dotenv": "^16.0.1",
    "graphql": "^15.5.0",
    "joi": "^17.6.0",
    "lodash.xor": "^4.5.0",
    "next": "^11.0.1",
    "next-auth": "^3.27.0",
    "next-pwa": "^5.0.5",
    "next-seo": "^5.5.0",
    "nextjs-progressbar": "^0.0.13",
    "polished": "^4.1.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-slick": "^0.28.1",
    "styled-components": "5.2.1",
    "styled-icons": "^10.29.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-typescript": "^7.13.0",
    "@jackfranklin/test-data-bot": "^1.3.0",
    "@storybook/addon-essentials": "6.1.21",
    "@storybook/react": "6.1.21",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.0.16",
    "@types/jest": "^26.0.20",
    "@types/next-auth": "^3.7.0",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-slick": "^0.23.4",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "apollo": "^2.32.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "cypress": "^10.8.0",
    "cypress-plugin-stripe-elements": "^1.0.2",
    "eslint": "^7.21.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.5.4",
    "msw": "^0.43.1",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.14.0",
    "storybook-addon-next-router": "^2.0.4",
    "typescript": "^4.2.3",
    "whatwg-fetch": "^3.6.2"
  }
}
